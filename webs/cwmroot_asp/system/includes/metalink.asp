<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<% If META_CurrentContentLanguage <> "" Then %>
<meta http-equiv="Content-Language" content="<%= META_CurrentContentLanguage %>">
<% End If %>
<meta http-equiv="Cache-Control" content="no-cache">
<meta http-equiv="Pragma" content="no-cache">
<meta http-equiv="Expires" content="0">
<META NAME="generator" CONTENT="camm WebManager V<%= System_Version %> Build <%= System_Build %> - <%= System_Licence %>">
<META NAME="publisher" CONTENT="<%= OfficialServerGroup_Company_FormerTitle %>">
<META NAME="author" CONTENT="<%= OfficialServerGroup_Company_FormerTitle %>">
<META NAME="copyright" CONTENT="<% = OfficialServerGroup_Company_FormerTitle %>">
<meta name="KEYWORDS" content="<%= META_KeywordContent %>">
<meta name="revisit-after" content="6 months">
<meta name="ROBOTS" content="INDEX, FOLLOW">
<% If META_DescriptionContent <> "" Then %>
<meta name="DESCRIPTION" content="<%= META_DescriptionContent %>">
<meta name="ABSTRACT" content="<%= META_DescriptionContent %>">
<% End If %>